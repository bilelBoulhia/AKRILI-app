<?xml version="1.0" encoding="utf-8"?>

<ScrollView  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#14171B">



    <FrameLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#14171B"
        tools:context=".fragments.Searchpostfragment">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/goBacksearch"
            android:layout_width="56dp"
            android:layout_height="48dp"
            android:background="@drawable/back_arrow"


            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck,VisualLintOverlap" />


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/showingPostimagessearch"
            android:layout_width="match_parent"
            android:layout_height="435dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="0dp"
            android:layout_marginTop="400dp">


            <TextView
                android:id="@+id/titleinpagesearch"
                android:layout_width="409dp"
                android:layout_height="33dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="1dp"
                android:layout_marginTop="41dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="369dp"

                android:fontFamily="@font/poppinsextrabold"

                android:text="laoding"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="411dp"
                android:layout_height="74dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="-1dp"
                android:layout_marginTop="81dp"
                android:layout_marginEnd="1dp"
                android:layout_marginBottom="288dp"
                android:background="@drawable/bordershapeforprofile"
                android:elevation="2dp"
                android:orientation="horizontal">


                <androidx.cardview.widget.CardView
                    android:layout_width="67dp"
                    android:layout_height="63dp"

                    app:cardCornerRadius="5dp">

                    <ImageView
                        android:id="@+id/pdpinpagesearch"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop"

                        />
                </androidx.cardview.widget.CardView>


                <TextView
                    android:id="@+id/usernameinpagesearch"
                    android:layout_width="wrap_content"
                    android:layout_height="32dp"

                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="15dp"
                    android:fontFamily="@font/poppinsextrabolitalic"
                    android:text="laoding"
                    android:textColor="@color/white"
                    android:textSize="17sp" />


            </LinearLayout>

            <androidx.cardview.widget.CardView
                android:layout_width="410dp"
                android:layout_height="279dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="164dp"
                android:layout_marginEnd="1dp"
                android:layout_marginBottom="0dp"
                android:scrollbarStyle="insideInset"
                app:cardBackgroundColor="#131920"

                app:cardCornerRadius="10dp">


                <TextView
                    android:id="@+id/loactioninpagesearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"


                    android:fontFamily="@font/poppinsextrabold"
                    android:padding="5dp"
                    android:text="laoding"
                    android:textColor="@color/white"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/cityinpagesearch"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"

                    android:layout_marginTop="50dp"
                    android:padding="5dp"
                    android:text="laoding"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:textStyle="bold" />


                <TextView
                    android:id="@+id/phoneinpagesearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginTop="100dp"

                    android:fontFamily="@font/poppinsextrabold"
                    android:padding="5dp"
                    android:text="laoding"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:textStyle="bold" />


                <TextView
                    android:id="@+id/descinpagesearch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginTop="180dp"
                    android:fontFamily="@font/poppinsextrabold"
                    android:padding="5dp"
                    android:scrollbars="vertical"
                    android:text="laoding"

                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    tools:ignore="TextSizeCheck" />


            </androidx.cardview.widget.CardView>


        </RelativeLayout>


    </FrameLayout>
























</ScrollView>













